{
    "@context":[
        "https://www.w3.org/2019/wot/td/v1",
        {
          "@language":"en"
        }
      ],
    "@type":"Thing",
    "title": "virtualDobot",
    "id": "urn:dev:ops:32473-virtualDobot",
    "description": "virtual magician dobot in coppeliasim",
    "security": "nosec_sc",
    "securityDefinitions": { 
      "nosec_sc": {
        "scheme": "nosec" 
      }
    },
    "actions": {
        "getCube":{
            "title":"get cube",
            "description":"Fetch the cube and put it on conveyor belt",
            "forms":[
                {
                "contentType":"application/json",
                "href":"http://localhost:9000/virtualdobot/actions/getCube",
                "htv:methodName":"POST",
                "op":["invokeaction"]
                }
            ]
        },
        "returnCube":{
            "title":"return cube",
            "description":"fetch cube from conveyor and return it back",
            "forms":[
                {
                "contentType":"application/json",
                "href":"http://localhost:9000/virtualdobot/actions/returnCube",
                "htv:methodName":"POST",
                "op":["invokeaction"]
                }
            ]
        }

    }
}